{
    "name": "staabm/rector-view-scope",
    "license": "MIT",
    "autoload": {
        "classmap": ["lib/"]
    },
    "autoload-dev": {
        "classmap": [
            "tests/classes/",
            "builtin/"
        ]
    },
    "require": {
        "php": ">=7.4",
        "rector/rector": "^0.10.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^0.12"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
