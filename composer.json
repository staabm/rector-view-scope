{
    "name": "staabm/rector-view-scope",
    "license": "MIT",
    "autoload": {
        "classmap": ["lib/"]
    },
    "autoload-dev": {
        "classmap": [
            "tests/classes/"
        ]
    },
    "require": {
        "php": ">=7.4",
        "rector/rector": "dev-fix-autolaod-in-tests as 0.10.12-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^0.12"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
